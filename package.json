{
  "name": "js-react-ctrl",
  "version": "0.0.1",
  "license": "LGPL-3.0",
  "homepage": "https://github.com/js-works/js-react-ctrl",
  "repository": {
    "type": "git",
    "url": "https://github.com/js-works/js-react-ctrl.git"
  },
  "main": "index.js",
  "module": "dist/js-react-ctrl.ems.production.js",
  "unpkg": "dist/js-react-ctrl.umd.production.js",
  "jsdelivr": "dist/js-react-ctrl.umd.production.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "index.js",
    "dist"
  ],
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "bestzip": "^2.1.2",
    "chai": "^4.2.0",
    "cli-watch": "^1.0.7",
    "copyfiles": "^2.1.0",
    "cross-conf-env": "^1.1.2",
    "esdoc": "^1.1.0",
    "esdoc-importpath-plugin": "^1.0.2",
    "esdoc-standard-plugin": "^1.0.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "react-dom": "16.7.0-alpha.2",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-gzip": "^2.2.0",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-serve": "^1.0.0",
    "rollup-plugin-terser": "^4.0.2",
    "rollup-plugin-typescript2": "^0.18.1",
    "rollup-plugin-uglify": "^6.0.1",
    "rollup-plugin-uglify-es": "0.0.1",
    "shx": "^0.3.2",
    "ts-loader": "^5.3.3",
    "ts-node": "^7.0.1",
    "typedoc": "^0.14.1",
    "typescript": "^3.2.2",
    "ziptool": "^1.0.2"
  },
  "dependencies": {
    "react": "16.7.0-alpha.2"
  },
  "nyc": {
    "temp-directory": "./coverage/.nyc_output",
    "include": [
      "src/modules"
    ],
    "exclude": [],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "all": true
  },
  "scripts": {
    "clean": "shx rm -rf ./build ./dist",
    "build": "rollup --c rollup.config.js && tsc -d --emitDeclarationOnly --declarationDir dist/types",
    "build-watch": "rollup --c rollup.config.js -w",
    "test": "nyc mocha -r ts-node/register -r source-map-support/register src/test/**/*.ts",
    "test-watch": "mocha -w -r ts-node/register -r source-map-support/register src/test/**/*.ts",
    "demo": "rollup --c rollup.demo.config.js -w",
    "docs": "shx rm -rf ./dist/docs && cross-conf-env typedoc --options ./typedoc.json --name \"js-react-ctrl (version npm_package_version)\" src/main/api",
    "zipsource": "shx rm -rf ./build/source && shx mkdir -p ./build/source && copyfiles -a ./* ./build/source && shx cp -r ./src ./build/source && shx mkdir -p ./dist/source && cd build/source && bestzip ../../dist/source/source.zip . && cd ../..",
    "dist": "npm run clean && npm run test && npm run build && npm run docs && npm run zipsource"
  }
}
